{
    "manifest_version": 3,
    "name": "Scalc+",
    "version": "2.5",
    "description": "An easy to use scientific calculator with extra functions",
    "permissions": [
        "clipboardRead"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "icons": {
        "128": "scr.png"
    },
    "action": {
        "default_icon": "scr.png",
        "default_popup": "calc/calc.html"
    }
}