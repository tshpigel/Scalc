{
    "manifest_version": 3,
    "name": "Scalc+",
    "version": "2.0",
    "description": "An easy to use scientific calculator with extra functions",
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "icons": {
        "128": "scr.png"
    },
    "action": {
        "default_icon": "scr.png",
        "default_popup": "main/calc.html"
    },
    "permissions": ["activeTab", "clipboardRead"]
}
